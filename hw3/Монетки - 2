// Разделяй и властвуй

#include "bits/stdc++.h"

using namespace std;

int m = 0;
long long mx = 0;

unordered_map<long long, int> sum;
vector<int> v;

void f(int pos, long long sm, int cnt) {
    if (pos == m) {
        auto tmp = sum[sm];
        sum[sm] = (tmp ? min(tmp, cnt) : cnt);

        if (sm > mx) {
            mx = sm;
        }
        return;
    }
    f(pos + 1, sm + v[pos], cnt + 1);
    f(pos + 1, sm, cnt);
}

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    int n = 0;
    cin >> n >> m;

    v = vector<int> (m);
    for (int i = 0; i < m; ++i) {
        cin >> v[i];
    }

    f(0, 0, 0);

    long long ans = 1e15;

    for (auto &it : sum) {
        if (sum[n - it.first]) {
            ans = min(ans, (long long) sum[n - it.first] + it.second);
        }
    }

    if (ans != (long long) 1e15) {
        cout << ans;
    } else if (2 * mx > n) {
        cout << 0;
    } else {
        cout << -1;
    }

    return 0;
}