#include "bits/stdc++.h"

using namespace std;

static const int INF = 1e9;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    freopen("sumdist.in", "r", stdin);
    freopen("sumdist.out", "w", stdout);

    int n = 0, m = 0;
    cin >> n >> m;
    vector<vector<int>> dist(n, vector<int>(n, INF));

    for (int i = 0; i < m; ++i) {
        int from = 0, to = 0;
        cin >> from >> to;
        from--;
        to--;
        dist[from][to] = 1;
        dist[to][from] = 1;
    }

    for (int k = 0; k < n; ++k) {
        for (int i = 0; i < n; ++i)
            if (i != k) {
                for (int j = 0; j < n; ++j)
                    if (j != i && j != k) {
                        dist[i][j] = min(dist[i][k] + dist[k][j], dist[i][j]);
                    }
            }
    }

    int ans = 0;
    for (int i = 0; i < n; ++i) {
        for (int j = 0; j < n; ++j) {
            ans += (dist[i][j] == INF ? 0 : dist[i][j]);
        }
    }

    cout << ans / 2;

    return 0;
}