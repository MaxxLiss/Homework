#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    long long n = 1790, k = 10, m = 1e18;
//    cin >> n >> k >> m;

    uint64_t prev = 1;
    uint64_t curr = k - 1;

    for (int i = 2; i <= n; ++i) {
        auto next = (curr * (k - 1) % m + prev * (k - 1) % m) % m;
        prev = curr;
        curr = next;
    }

    cout << curr;

    return 0;
}