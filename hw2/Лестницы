#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    int n = 0;
    cin >> n;

    // i - высота посл. столбца, j - количество кубиков
    vector<vector<uint64_t>> dp(n + 1, vector<uint64_t> (n + 1, 0));
    for (int i = 1; i <= n; ++i) {
        dp[i][i] = 1;
    }
    ::uint64_t ans = 0;
    for (int i = 2; i < n; ++i) {
        for (int j = i + 1; j <= n; ++j) {
            for (int k = 1; k <= i - 1; ++k) {
                dp[i][j] += dp[k][j - i];
            }
            if (j == n) {
                ans += dp[i][j];
            }
        }
    }

//    for (auto &a : dp) {
//        for (auto &it : a) {
//            cout << it << " ";
//        }
//        cout << endl;
//    }

    cout << ans;

    return 0;
}

