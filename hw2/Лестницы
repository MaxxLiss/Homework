//todo dp[i][j] - количество способов поставить в i столбец j кубиков
//todo dp[i][j] = sum(dp[i - 1][1..j - 1])
//todo ответ: sum(dp[n][2..n - 1]

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    int n = 0;
    cin >> n;

//    f(n, 1, n);

    vector<vector<uint64_t>> dp(n + 1, vector<uint64_t> (n + 1, 0));
    for (int j = 1; j <= n; ++j) {
        dp[1][j]++;
    }
    for (int i = 2; i <= n; ++i) {
        for (int j = 2; j <= n; ++j) {
            for (int k = 1; k <= j; ++k) {
                dp[i][j] += dp[i - 1][k];
            }
        }
    }

    for (auto &a : dp) {
        for (auto it : a) {
            cout << it << " ";
        }
        cout << endl;
    }

    uint64_t ans = 0;
    for (int i = 2; i < n; ++i) {
        ans += dp[n][i];
    }

    cout << ans;

    return 0;
}