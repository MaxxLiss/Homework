//}


int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    cin >> n >> c;
    v = vector<int> (n);

    for (int i = 0; i < n; ++i) {
        cin >> v[i];
    }

    f(0, 0);

    cout << (ans <= 0.01 ? "YES" : "NO");

    return 0;
}