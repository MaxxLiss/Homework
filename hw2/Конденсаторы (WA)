#include "bits/stdc++.h"

using namespace std;

int n = 0;
double ans = 1e9, c = 0;

vector<bool> used;
vector<int> v;

void f(double curr_c) {
    if (abs(c - curr_c) < ans) {
        ans = abs(c - curr_c);
    }
    for (int i = 0; i < n; ++i) {
        if (used[i]) {
            continue;
        }
        used[i] = true;
        double posl = v[i] + curr_c;
        double parral = (v[i] * curr_c) / posl;
        f(posl);
        f(parral);
        used[i] = false;
    }
}

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    cin >> n >> c;
    v = vector<int> (n);
    used = vector<bool> (n, false);

    for (int i = 0; i < n; ++i) {
        cin >> v[i];
    }

    f(0);

    cout << (ans <= 0.01 ? "YES" : "NO");

    return 0;
}