#include "bits/stdc++.h"

using namespace std;

int n = 0;
double ans = 1e9, c = 0;

vector<int> v;

void f(int curr, double curr_c) {
    if (abs(c - curr_c) < ans) {
        ans = abs(c - curr_c);
    }
    for (int i = curr; i < n; ++i) {
        double posl = v[i] + curr_c;
        double parral = (v[i] * curr_c) / posl;
        f(i + 1, abs(c - posl) < abs(c - parral) ? posl : parral);
    }
}

