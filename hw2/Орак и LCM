#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    unordered_map<int, pair<int, pair<int, int>>> m;
    // m[простое число]={количество чисел с таким делителем, {минимум таких делителей в каком-то числе, второй минимум}}

    int n = 0;
    cin >> n;

    for (int i = 0; i < n; ++i) {
        int a = 0;
        cin >> a;
        unordered_map<int, int> mi;
        int del = 2;
        while (a > 1) {
            if (!(a % del)) {
                mi[del]++;
                a /= del;
            } else {
                del++;
            }
        }
        for (auto &it : mi) {
            auto& up_data = m[it.first];
            up_data.first++;
            if ((it.second < up_data.second.second) || !up_data.second.second) {
                if ((it.second < up_data.second.first) || !up_data.second.first) {
                    up_data.second.second = up_data.second.first;
                    up_data.second.first = it.second;
                } else {
                    up_data.second.second = it.second;
                }
            }
        }
    }

    int ans = 1;

    for (auto &it : m) {
        if (it.second.first == n - 1) {
            ans *= pow(it.first, it.second.second.first);
        } else if (it.second.first == n) {
            if (!it.second.second.second) {
                ans *= pow(it.first, it.second.second.first);
            } else {
                ans *= pow(it.first, it.second.second.second);
            }
        }
    }

    cout << ans;

    return 0;
}