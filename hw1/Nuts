#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    int n = 0, ans = 0;
    cin >> n;

    for (int i = 0; i < n; ++i) {
        int a;
        cin >> a;
        if (a > 10) {
            ans += a - 10;
        }
    }

    cout << ans;

    return 0;
}