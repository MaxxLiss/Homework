#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    int n = 0, k = 0, ans = 0;
    cin >> n >> k;

    ans = (int)((n > 1 ? (1 + n) / 2.0 * n * k * 100 : k * 100) + (k > 1 ? (1 + k) / 2.0 * k : 1) * n);

    cout << ans;

    return 0;
}