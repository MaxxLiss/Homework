#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    double n = 0;
    cin >> n;
    int price = (int) (1.08 * n);
    if (price < 206) {
        cout << "Yay!";
    } else if (price > 206) {
        cout << ":(";
    } else {
        cout << "so-so";
    }

    return 0;
}