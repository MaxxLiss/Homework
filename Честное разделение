#include "bits/stdc++.h"

using namespace std;

int main() {
    cin.tie(nullptr); cout.tie(nullptr);
    ios_base::sync_with_stdio(false);

    int t = 0;
    cin >> t;

    while (t--) {
        int n = 0;
        cin >> n;

        map<int, int> m;
        for (int i = 0; i < n; ++i) {
            int a = 0;
            cin >> a;
            m[a]++;
        }
        bitset<250> dp;
        dp[0] = true;
        for (auto &it: m) {
            while (it.second >= 3) {
                it.second -= 2;
                m[it.first * 2]++;
            }
            dp |= (dp << it.first);
            if (it.second > 1) {
                dp |= (dp << it.first);
            }
        }
        int mx;
        for (mx = 249; mx >= 0; --mx) {
            if (dp[mx]) {
                break;
            }
        }
        cout << (!(mx & 1) && dp[mx / 2] ? "YES\n" : "NO\n");
    }

    return 0;
}